{
    "openapi": "3.0.2",
    "info": {
        "title": "Program API",
        "description": "Program API",
        "version": "1.0"
    },
    "servers": [{ "url": "https://localhost:8000" }],
    "paths": {
        "/api/v1/program": {
            "post": {
                "description": "Membuat Program",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "requestBody": {
                    "description": "Membuat Program",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "judul": {
                                        "type": "string"
                                    },
                                    "deskripsi": {
                                        "type": "string"
                                    },
                                    "kategori":{
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Berhasil membuat kelengkapan dokumen",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "number"
                                                },
                                                "judul": {
                                                    "type": "string"
                                                },
                                                "deskripsi": {
                                                    "type": "string"
                                                },
                                                "kategori": {
                                                    "type": "string"
                                                },
                                                "created_at": {
                                                    "type": "string"
                                                },
                                                "updated_at":{
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Gagal membuat berita",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "validationerror": {
                                        "description": "Validation Error",
                                        "value": {
                                            "errors": {
                                                "judul": [
                                                    "tanggal is required"
                                                ],
                                                "deskripsi": [
                                                    "penulis is required"
                                                ],
                                                "kategori": [
                                                    "judul is required"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "get": {
                "description": "Search Program",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    },
                    {
                        "name": "judul",
                        "in": "query"
                    },
                    {
                        "name": "kategori",
                        "in": "query"
                    }

                ],
                "responses": {
                    "200": {
                        "description": "Success Search Program",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "number"
                                                },
                                                "judul": {
                                                    "type": "string"
                                                },
                                                "deskripsi": {
                                                    "type": "string"
                                                },
                                                "kategori": {
                                                    "type": "string"
                                                },
                                                "created_at": {
                                                    "type": "string"
                                                },
                                                "updated_at": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/program/{id}":{
            "put":{
                "description": "Update Program",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    },
                    {
                        "name": "id",
                        "in": "path"
                    }
                ],
                "requestBody":{
                    "description": "Update Program",
                    "content": {
                        "application/json":{
                            "schema":{
                                "type": "object",
                                "properties": {
                                    "judul": {
                                        "type": "string"
                                    },
                                    "deskripsi": {
                                        "type": "string"
                                    },
                                    "kategori": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200":{
                        "description": "Success Update Data Program",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "number"
                                                },
                                                "judul": {
                                                    "type": "string"
                                                },
                                                "deskripsi": {
                                                    "type": "string"
                                                },
                                                "kategori": {
                                                    "type": "string"
                                                },
                                                "created_at": {
                                                    "type": "string"
                                                },
                                                "updated_at":{
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description": "Gagal membuat kelengkapan dokumen",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "validationerror": {
                                        "description": "Validation Error",
                                        "value": {
                                            "errors": {
                                                "judul": [
                                                    "judul is required"
                                                ],
                                                "deskripsi": [
                                                    "description is required"
                                                ],
                                                "kategori": [
                                                    "kategori is required"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete":{
                "description": "Delete Program",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    },
                    {
                        "name": "id",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200":{
                        "description": "Success delete Program",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "data":{
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
