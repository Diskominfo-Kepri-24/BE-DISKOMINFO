{
    "openapi": "3.0.2",
    "info": {
        "title": "Laporan API",
        "description": "API untuk mengelola laporan",
        "version": "1.0.0"
    },
    "components": {
        "schemas": {
            "Berita": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "tanggal": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-07-22"
                    },
                    "slug": {
                        "type": "string",
                        "example": "judul-berita-unik"
                    },
                    "judul": {
                        "type": "string",
                        "example": "Judul Berita"
                    },
                    "isi_berita": {
                        "type": "string",
                        "example": "Isi berita yang sangat menarik."
                    },
                    "kategori": {
                        "type": "string",
                        "example": "Olahraga"
                    },
                    "gambar": {
                        "type": "string",
                        "example": "storage/berita/1626949200_gambar_judul-berita-unik.jpg"
                    },
                    "id_user": {
                        "type": "integer",
                        "example": 1
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-07-22T10:00:00.000000Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-07-22T10:00:00.000000Z"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "security": [
        {
            "bearerAuth": []
        }
    ],
    "servers": [{ "url": "http://localhost:8000" }],
    "paths": {
        "/api/v1/laporan": {
            "get": {
                "summary": "Get all laporan (untuk role pembimbing)",
                "description": "Endpoint untuk mendapatkan semua laporan",
                "responses": {
                    "200": {
                        "description": "Daftar semua laporan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "laporan": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "user_id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "link_laporan": {
                                                        "type": "string",
                                                        "example": "https://example.com/laporan.pdf"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-07-29T14:32:00Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-07-29T14:32:00Z"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Tambah Laporan (untuk role mahasiswa/siswa)",
                "description": "Endpoint untuk menambahkan laporan baru",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "link_laporan": {
                                        "type": "string",
                                        "example": "http://example.com/laporan.pdf"
                                    }
                                },
                                "required": ["link_laporan"]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Data laporan berhasil ditambah",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "laporan": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "user_id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "link_laporan": {
                                                    "type": "string",
                                                    "example": "http://example.com/laporan.pdf"
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-07-29T12:34:56.000000Z"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-07-29T12:34:56.000000Z"
                                                }
                                            }
                                        },
                                        "status": {
                                            "type": "string",
                                            "example": "Data Laporan berhasil ditambah"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validasi error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "type": "object",
                                            "properties": {
                                                "link_laporan": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "The link laporan field is required."
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "summary": "Update laporan magang (untuk role mahasiswa/siswa)",
                "description": "Endpoint untuk mengubah data laporan magang",
                "tags": ["Laporan"],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "link_laporan": {
                                        "type": "string",
                                        "example": "https://link-to-report.com"
                                    }
                                },
                                "required": ["link_laporan"]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Data laporan berhasil diubah",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "laporan": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "user_id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "link_laporan": {
                                                    "type": "string",
                                                    "example": "https://link-to-report.com"
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2023-01-01T00:00:00.000000Z"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2023-01-01T00:00:00.000000Z"
                                                }
                                            }
                                        },
                                        "status": {
                                            "type": "string",
                                            "example": "Data laporan berhasil diubah"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validasi gagal",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "type": "object",
                                            "properties": {
                                                "link_laporan": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "The link laporan field is required."
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "summary": "Delete laporan (untuk role mahasiswa/siswa)",
                "description": "Endpoint untuk menghapus laporan user yang terautentikasi",
                "tags": ["Laporan"],
                "responses": {
                    "200": {
                        "description": "Data laporan berhasil dihapus",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "Data laporan berhasil dihapus"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Laporan tidak ditemukan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Laporan not found."
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v1/laporan/magang": {
            "get": {
                "summary": "Get laporan magang for authenticated user (untuk role mahasiswa/siswa)",
                "description": "Endpoint untuk mendapatkan laporan magang pengguna yang diautentikasi",
                "responses": {
                    "200": {
                        "description": "Laporan magang untuk pengguna yang diautentikasi",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "laporan": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "user_id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "link_laporan": {
                                                    "type": "string",
                                                    "example": "https://example.com/laporan.pdf"
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-07-29T14:32:00Z"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-07-29T14:32:00Z"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Laporan tidak ditemukan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Laporan not found"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
