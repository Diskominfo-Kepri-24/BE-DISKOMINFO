{
    "openapi": "3.0.2",
    "info": {
        "title": "Agenda API",
        "description": "Agenda API",
        "version": "1.0"
    },
    "components": {
        "schemas": {
            "Agenda": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "judul": {
                        "type": "string"
                    },
                    "slug": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    },
                    "tanggal_mulai": {
                        "type": "string"
                    },
                    "tanggal_selesai": {
                        "type": "string"
                    },
                    "tipe_acara": {
                        "type": "string"
                    },
                    "isi_agenda": {
                        "type": "string"
                    },
                    "foto": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "security": [
        {
            "bearerAuth": []
        }
    ],
    "servers": [{ "url": "http://localhost:8000" }],
    "paths": {
        "/api/v1/agenda": {
            "get": {
                "summary": "Get all agendas",
                "description": "Endpoint untuk mendapatkan daftar semua agenda",
                "responses": {
                    "200": {
                        "description": "Daftar agenda berhasil diambil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "agenda": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Agenda"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Add a new agenda",
                "description": "Endpoint untuk menambahkan agenda baru",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "judul": {
                                        "type": "string",
                                        "description": "Judul agenda"
                                    },
                                    "slug": {
                                        "type": "string",
                                        "description": "Slug agenda"
                                    },
                                    "status": {
                                        "type": "string",
                                        "description": "Status agenda"
                                    },
                                    "tanggal_mulai": {
                                        "type": "string",
                                        "format": "date",
                                        "description": "Tanggal mulai agenda"
                                    },
                                    "tanggal_selesai": {
                                        "type": "string",
                                        "format": "date",
                                        "description": "Tanggal selesai agenda"
                                    },
                                    "tipe_acara": {
                                        "type": "string",
                                        "description": "Tipe acara"
                                    },
                                    "isi_agenda": {
                                        "type": "string",
                                        "description": "Isi agenda"
                                    },
                                    "gambar": {
                                        "type": "string",
                                        "format": "binary",
                                        "description": "Gambar agenda"
                                    }
                                },
                                "required": [
                                    "judul",
                                    "slug",
                                    "status",
                                    "tanggal_mulai",
                                    "tanggal_selesai",
                                    "tipe_acara",
                                    "isi_agenda"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Data agenda berhasil ditambah",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "agenda": {
                                            "$ref": "#/components/schemas/Agenda"
                                        },
                                        "status": {
                                            "type": "string",
                                            "example": "Data agenda berhasil ditambah"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/agenda/{slug}": {
            "get": {
                "summary": "Get agenda by slug",
                "description": "Endpoint untuk mendapatkan agenda berdasarkan slug",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "required": true,
                        "description": "Slug dari agenda yang ingin diambil",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Agenda berhasil diambil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "agenda": {
                                            "$ref": "#/components/schemas/Agenda"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Agenda tidak ditemukan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Agenda not found"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Update agenda by slug",
                "description": "Endpoint untuk mengupdate agenda berdasarkan slug",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "required": true,
                        "description": "Slug dari agenda yang ingin diupdate",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "judul": {
                                        "type": "string",
                                        "description": "Judul agenda"
                                    },
                                    "slug": {
                                        "type": "string",
                                        "description": "Slug agenda"
                                    },
                                    "status": {
                                        "type": "string",
                                        "description": "Status agenda"
                                    },
                                    "tanggal_mulai": {
                                        "type": "string",
                                        "format": "date",
                                        "description": "Tanggal mulai agenda"
                                    },
                                    "tanggal_selesai": {
                                        "type": "string",
                                        "format": "date",
                                        "description": "Tanggal selesai agenda"
                                    },
                                    "tipe_acara": {
                                        "type": "string",
                                        "description": "Tipe acara agenda"
                                    },
                                    "isi_agenda": {
                                        "type": "string",
                                        "description": "Isi agenda"
                                    },
                                    "gambar": {
                                        "type": "string",
                                        "format": "binary",
                                        "description": "Gambar agenda"
                                    }
                                },
                                "required": [
                                    "judul",
                                    "status",
                                    "slug",
                                    "tanggal_mulai",
                                    "tanggal_selesai",
                                    "tipe_acara",
                                    "isi_agenda"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Agenda berhasil diubah",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "agenda": {
                                            "$ref": "#/components/schemas/Agenda"
                                        },
                                        "status": {
                                            "type": "string",
                                            "example": "Agenda berhasil diubah"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Agenda tidak ditemukan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Agenda not found"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validasi error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "summary": "Delete agenda by slug",
                "description": "Endpoint untuk menghapus agenda berdasarkan slug",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "required": true,
                        "description": "Slug dari agenda yang ingin dihapus",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Agenda berhasil dihapus",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Agenda tidak ditemukan",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Agenda not found"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
